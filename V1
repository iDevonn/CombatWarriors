--
    -- PlayerTab
    local PlayerTab = Window:MakeTab({
        Name = "TROLL MENU",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

            --PlayerButtons
            PlayerTab:AddButton({
                Name = "InfiniteYield",
                Callback = function()
                          InfiniteYield()
                  end    
            })

            PlayerTab:AddButton({
                Name = "Animation1",
                Callback = function()
                          Animation1()
                  end    
