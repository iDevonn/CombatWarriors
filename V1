    Name = "Glory Nearby",
    Default = false,
    Callback = function(Value)
    _G.GloryNearby = Value
    GloryNearby()
    end
    })

--

PlayerTab:AddToggle({
    Name = "Farm Kills",
    Default = false,
    Callback = function(Value)
    _G.FarmKills = Value
    FarmKills()
    end
})
   
